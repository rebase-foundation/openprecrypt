{
   "name": "precrypt",
   "version": "0.3.5",
   "description": "Cli wrapper for the umbral-pre proxy re-encryption library with parallel processing.",
   "author": "Jacob Peddicord <jacob.peddicord@hey.com>",
   "repository": "https://github.com/rebase-foundation/precrypt/tree/main/precrypt",
   "homepage": "https://precrypt.org",
   "license": "GPL-3.0-only",
   "bin": {
     "swp": "npm/run.js"
   },
   "scripts": {
     "preinstall": "node npm/scripts.js uninstall",
     "postinstall": "node npm/scripts.js install",
     "docs:dev": "vuepress dev docs",
     "docs:build": "vuepress build docs && ncp docs/_redirects docs/.vuepress/dist/_redirects"
   },
   "dependencies": {
     "binary-install": "^0.0.1"
   },
   "devDependencies": {
     "ncp": "^2.0.0",
     "vuepress": "^1.5.4"
   },
   "keywords": [
     "cli",
     "crypto"
   ],
   "files": [
     "npm/**/*"
   ]
 }